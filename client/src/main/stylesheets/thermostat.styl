.thermostatIcon
  background-image url(/stylesheets/img/thermostat.png)
  background-size cover
  height 3em
  width 3em
  display block
.thermostat
  .frame-face
    position:relative;
    width:30em;
    height:30em;
    margin:2em auto;
    border-radius:15em;
    background:-webkit-linear-gradient(top, #f9f9f9,#666);
    background:-moz-linear-gradient(top, #f9f9f9,#666);
    background:linear-gradient(to bottom, #f9f9f9,#666);
    box-shadow:rgba(0,0,0,.8) .5em .5em 4em;

  .frame-face:before
    content:'';
    width:29.4em;
    height:29.4em;
    border-radius:14.7em;
    position:absolute;
    top:.3em; left:.3em;
    background:
      -webkit-linear-gradient(135deg, rgba(246,248,249,0) 0%,rgba(229,235,238,1) 50%,rgba(205,212,217,1) 51%,rgba(245,247,249,0) 100%),
      -webkit-radial-gradient(center, ellipse cover, rgba(246,248,249,1) 0%,rgba(229,235,238,1) 65%,rgba(205,212,217,1) 66%,rgba(245,247,249,1) 100%);
    background:
      -moz-linear-gradient(135deg, rgba(246,248,249,0) 0%,rgba(229,235,238,1) 50%,rgba(205,212,217,1) 51%,rgba(245,247,249,0) 100%),
      -moz-radial-gradient(center, ellipse cover, rgba(246,248,249,1) 0%,rgba(229,235,238,1) 65%,rgba(205,212,217,1) 66%,rgba(245,247,249,1) 100%);
    background:
      linear-gradient(135deg, rgba(246,248,249,0) 0%,rgba(229,235,238,1) 50%,rgba(205,212,217,1) 51%,rgba(245,247,249,0) 100%),
      radial-gradient(ellipse at center, rgba(246,248,249,1) 0%,rgba(229,235,238,1) 65%,rgba(205,212,217,1) 66%,rgba(245,247,249,1) 100%);

  .frame-face:after
    content:'';
    width:28em;
    height:28em;
    border-radius:14.2em;
    position:absolute;
    top:.9em; left:.9em;
    box-shadow:inset rgba(0,0,0,.2) .2em .2em 1em;
    border:.1em solid rgba(0,0,0,.2);
    background:-webkit-linear-gradient(top, #fff, #ccc);
    background:-moz-linear-gradient(top, #fff, #ccc);
    background:linear-gradient(to bottom, #fff, #ccc);
  .color-circle-heat
    background-image: -moz-linear-gradient(45deg, #871F0B 0%, #E45A0D 100%);
    background-image: -o-linear-gradient(45deg, #871F0B 0%, #E45A0D 100%);
    background-image: -webkit-linear-gradient(45deg, #871F0B 0%, #E45A0D 100%);
    background-image: linear-gradient(45deg, #871F0B 0%, #E45A0D 100%);

  .color-circle-cool
    background-image: -moz-linear-gradient(45deg, #0b1c87 0%, #0dd7e4 100%);
    background-image: -o-linear-gradient(45deg, #0b1c87 0%, #0dd7e4 100%);
    background-image: -webkit-linear-gradient(45deg, #0b1c87 0%, #0dd7e4 100%);
    background-image: linear-gradient(45deg, #0b1c87 0%, #0dd7e4 100%);

  .color-circle
    position: absolute;
    top: 55px;
    margin-left: 56px;
    z-index: 2;
    width: 310px;
    height: 310px;
    -moz-border-radius: 153px;
    -webkit-border-radius: 153px;
    border-radius: 153px;
    -moz-background-clip: padding;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    background-color: #f5f5f5;
    -moz-box-shadow: inset 0 0 10px rgba(0,0,0,.36), 0 0 0 1px #969696;
    -webkit-box-shadow: inset 0 0 10px rgba(0,0,0,.36), 0 0 0 1px #969696;
    box-shadow: inset 0 0 10px rgba(0,0,0,.36), 0 0 0 1px #969696;

  .text-readout
    color: #f5f5f5;
    text-shadow: 0 2px 4px rgba(3,4,4,.55);
    font-size: 11em;
    font-family: "Myriad Pro Regular",sans-serif;
    font-weight: 700;

    position:relative;
    top:-48px;
    /*left:116px;*/
    /*margin-left: 115px;*/
    z-index:4;
    text-align: center;

  .thermostat-target-needle
    display:block;
    width:.4em;
    height:2.5em;
    background:red;
    position:absolute;
    top:50%; left:50%;
    margin:-1.1em 0 0 -.2em;
    // transform:rotate(12deg)   translateY(-12.7em)

  .thermostat-current-needle
    display:block;
    width:.4em;
    height:2.5em;
    background:black;
    position:absolute;
    top:50%; left:50%;
    margin:-1.1em 0 0 -.2em;
    // transform:rotate(22deg)   translateY(-12.7em)

  .thermostat-current-number
    display: block;
    width: 1.2em;
    font-size: 20pt;
    height: 1em;
    position: absolute;
    top: 50%;
    font-weight: bold;
    left: 50%;
    z-index: 2;
    // transform:rotate(22deg)   translateY(-12.7em)

  ul
    list-style:none;
    margin:0;
    padding:0
  .minute-marks li
    display:block;
    width:.2em;
    height:.6em;
    background:#929394;
    position:absolute;
    top:50%; left:50%;
    margin:-.4em 0 0 -.1em;

    &:first-child {transform:rotate(6deg)     translateY(-12.7em)}
    &:nth-child(2) {transform:rotate(12deg)   translateY(-12.7em)}
    &:nth-child(3) {transform:rotate(18deg)   translateY(-12.7em)}
    &:nth-child(4) {transform:rotate(24deg)   translateY(-12.7em)}
    &:nth-child(5) {transform:rotate(30deg)   translateY(-12.7em)}
    &:nth-child(6) {transform:rotate(36deg)   translateY(-12.7em)}
    &:nth-child(7) {transform:rotate(42deg)   translateY(-12.7em)}
    &:nth-child(8) {transform:rotate(48deg)   translateY(-12.7em)}
    &:nth-child(9) {transform:rotate(54deg)   translateY(-12.7em)}
    &:nth-child(10) {transform:rotate(60deg)   translateY(-12.7em)}
    &:nth-child(11) {transform:rotate(66deg)   translateY(-12.7em)}
    &:nth-child(12) {transform:rotate(72deg)  translateY(-12.7em)}
    &:nth-child(13) {transform:rotate(78deg)  translateY(-12.7em)}
    &:nth-child(14) {transform:rotate(84deg)  translateY(-12.7em)}
    &:nth-child(15) {transform:rotate(90deg)  translateY(-12.7em)}
    &:nth-child(16) {transform:rotate(96deg)  translateY(-12.7em)}
    &:nth-child(17) {transform:rotate(102deg) translateY(-12.7em)}
    &:nth-child(18) {transform:rotate(108deg) translateY(-12.7em)}
    &:nth-child(19) {transform:rotate(114deg) translateY(-12.7em)}
    &:nth-child(20) {transform:rotate(120deg) translateY(-12.7em)}
    &:nth-child(21) {transform:rotate(126deg) translateY(-12.7em)}
    &:nth-child(22) {transform:rotate(132deg) translateY(-12.7em)}
    &:nth-child(23) {transform:rotate(138deg) translateY(-12.7em)}
    &:nth-child(24) {transform:rotate(144deg) translateY(-12.7em)}
    &:nth-child(25) {transform:rotate(216deg) translateY(-12.7em)}
    &:nth-child(26) {transform:rotate(222deg) translateY(-12.7em)}
    &:nth-child(27) {transform:rotate(228deg) translateY(-12.7em)}
    &:nth-child(28) {transform:rotate(234deg) translateY(-12.7em)}
    &:nth-child(29) {transform:rotate(240deg) translateY(-12.7em)}
    &:nth-child(30) {transform:rotate(246deg) translateY(-12.7em)}
    &:nth-child(31) {transform:rotate(252deg) translateY(-12.7em)}
    &:nth-child(32) {transform:rotate(258deg) translateY(-12.7em)}
    &:nth-child(33) {transform:rotate(264deg) translateY(-12.7em)}
    &:nth-child(34) {transform:rotate(270deg) translateY(-12.7em)}
    &:nth-child(35) {transform:rotate(276deg) translateY(-12.7em)}
    &:nth-child(36) {transform:rotate(282deg) translateY(-12.7em)}
    &:nth-child(37) {transform:rotate(288deg) translateY(-12.7em)}
    &:nth-child(38) {transform:rotate(294deg) translateY(-12.7em)}
