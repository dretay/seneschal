    &:nth-child(43) {transform:rotate(324deg) translateY(-12.7em)}
    &:nth-child(44) {transform:rotate(330deg) translateY(-12.7em)}
    &:nth-child(45) {transform:rotate(336deg) translateY(-12.7em)}
    &:nth-child(46) {transform:rotate(342deg) translateY(-12.7em)}
    &:nth-child(47) {transform:rotate(348deg) translateY(-12.7em)}
    &:nth-child(48) {transform:rotate(354deg) translateY(-12.7em)}
    &:nth-child(49) {transform:rotate(0deg) translateY(-12.7em)}
  .digits
    width:30em;
    height:30em;
    border-radius:15em;
    position:absolute;
    top:48px; left:50%;
    margin-left:-15em;

    li
      font-size:1.6em;
      display:block;
      width:1.6em;
      height:1.6em;
      position:absolute;
      top:50%; left:50%;
      line-height:1.6em;
      text-align:center;
      margin:-.8em 0 0 -.8em;
      font-weight:bold;

      &:nth-child(1) { transform:translate(3.9em, -6.9em) }
      &:nth-child(2) { transform:translate(6.9em, -4em) }
      &:nth-child(3) { transform:translate(8em, 0) }
      &:nth-child(4) { transform:translate(6.8em, 4em) }
      &:nth-child(5) { transform:translate(3.9em, 6.9em) }
      &:nth-child(6) { transform:translate(0, 8em) }
      &:nth-child(7) { transform:translate(-3.9em, 6.9em) }
      &:nth-child(8) { transform:translate(-6.8em, 4em) }
      &:nth-child(9) { transform:translate(-8em, 0) }
      &:nth-child(10) { transform:translate(-6.9em, -4em) }
      &:nth-child(11) { transform:translate(-3.9em, -6.9em) }
      &:nth-child(12) { transform:translate(0, -8em) }


  .hvacInfo
    font-size 20pt
    position relative
    top -92px
    text-transform uppercase

  .thermostatControlTop
    cursor: pointer;
    position: relative;
    top: 0;
    font-weight: 100;
    z-index: 1;
    /*display: block;*/

  .thermostatControlBottom
    cursor: pointer;
    position: relative;
    bottom: 260px;
    font-weight: 100;
    z-index: 1;

    /*display: block;*/

  .text-readout .degree
    font-weight: 400;
    // padding: 50px 0 10px 0;
    top: -140px;
    left: 15px;
    position: relative;
    cursor: default;
    font-family: "Myriad Pro Bold Condensed";
